{
	"name": "isomorphic-graphs",
	"version": "0.0.0",
	"license": "MIT",
	"bin": "dist/cli.js",
	"engines": {
		"node": ">=10"
	},
	"scripts": {
		"build": "tsc",
		"start": "yarn build && node ./dist/cli.js",
		"compile": "yarn build && pkg -C GZip -t node16-windows-x64 -o ./exec/isomorph ./dist/cli.js"
	},
	"files": [
		"dist/cli.js"
	],
	"dependencies": {
		"ink": "^3.2.0",
		"meow": "^9.0.0",
		"react": "^17.0.2"
	},
	"pkg": {
		"targets": [ "node16-windows-x64" ],
		"output": "./exec"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^2.0.0",
		"@types/node": "^16.11.6",
		"@types/react": "^17.0.34",
		"chalk": "^4.1.2",
		"eslint-config-xo-react": "^0.25.0",
		"eslint-plugin-react": "^7.26.1",
		"eslint-plugin-react-hooks": "^4.2.0",
		"typescript": "^4.4.4"
	}
}
